    <declare-styleable name="MessageTextView">
        <attr name="typing_src" format="reference" />
        <attr name="like_src" format="reference" />
        <attr name="text_color" format="reference" />
        <attr name="text_size" format="dimension" />
    </declare-styleable>
</resources>